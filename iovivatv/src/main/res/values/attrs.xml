<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PageIndicator">
        <attr name="selectIndex" format="integer" />
        <attr name="multiSelectable" format="boolean" />
        <attr name="itemCount" format="integer" />
        <attr name="itemDrawable" format="reference" />
    </declare-styleable>
    
    <declare-styleable name="Gallery">
        <attr name="index" format="integer" />
        <attr name="animationDuration" format="integer" />
        <attr name="spacing" format="integer" />
        <attr name="unselectedAlpha" format="float" />
    </declare-styleable>

</resources>